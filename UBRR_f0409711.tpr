     , s.RegNumber
     , s.Number   
     , f.ISONumber       
     , CASE WHEN portf.PortfolioID = @Portf5Kat/*'5-я категория'*/ THEN 0
            ELSE kt.Num END
     , CASE WHEN portf.PortfolioID = @Portf5Kat /*'5-я категория'*/ THEN kt.Num
