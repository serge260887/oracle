@SQL_3@ = SQL{
-----------------------------
--   711 форма Раздел 3    --
-----------------------------
DECLARE @KvitID DSIDENTIFIER
      , @Date   DSOPERDAY   
      , @Portf5Kat DSIDENTIFIER                                
                  
SELECT @Date = %DATE! 
SELECT @Date = @Date - 1    

SELECT @KvitID = 10000000000

-- Заполним параметры дял ВУЗа     
IF EXISTS ( SELECT 'X'
              FROM [dbo].[tInstitution] 
             WHERE InstitutionID = 2000
               AND Brief LIKE '\%ВУЗ-БАНК\%')
  SELECT @KvitID = 19   
  
--Найдем портфель 5-ой категории
SELECT @Portf5Kat = PortfolioID
  FROM tPortfolio 
 WHERE Brief LIKE '5\%категория'  


INSERT INTO #Rep3 (InstitutionID 
                 , SecurityID  
                 , PortfolioID  
                 , InstName     
                 , INN             
                 , RegNumber     
                 , ISIN         
                 , Repo     
                  )
SELECT s.InstitutionID    
     , s.SecurityID   
     , kp.PortfolioID
     , i.Name
     , i.INN                               
     , s.RegNumber
     , s.Number   
     , kt.Num
  FROM tKvitTotal kt
 INNER JOIN tKvitPosition kp
    ON kp.KvitPositionID = kt.KvitPositionID
 INNER JOIN tPortfolio portf
    ON portf.PortfolioID = kp.PortfolioID
 INNER JOIN tSecurity s
    ON s.SecurityID = kp.SecurityID   
 INNER JOIN tInstitution i
    ON i.InstitutionID = s.InstitutionID   
 WHERE kt.Date = @Date 
   AND kt.Num > 0
   AND kt.Type = 14     
   AND kp.KvitID = @KvitID          
   AND kp.PortfolioID <> @Portf5Kat
        
-- Сделки обратного РЕПО                      
INSERT INTO #Rep3 (InstitutionID 
                 , SecurityID  
                 , PortfolioID  
                 , InstName     
                 , INN               
                 , RegNumber     
                 , ISIN         
                 , RepoObr 
                 , RepoObrZaem                    
                  )     
SELECT i.InstitutionID
     , s.SecurityID
     , d1.LtPortfolioID 
     , i.Name
     , i.INN
     , s.RegNumber
     , s.Number  
     , SUM(CASE WHEN (d1.CalcNKS & 256) / 256 = 0 THEN d1.Num ELSE 0 END) AS RepoObr
     , SUM(CASE WHEN (d1.CalcNKS & 256) / 256 = 1 THEN d1.Num ELSE 0 END) AS RepoObrZaem                                                 
  FROM tDealRelation dr 
 INNER JOIN tDeal d1
    ON d1.DealID = dr.ParentID
 INNER JOIN tDealTransact dt1
    ON dt1.DealID = d1.DealID
   AND dt1.TransactType = 4
 INNER JOIN tDeal d2
    ON d2.DealID = dr.ChildID
 INNER JOIN tDealTransact dt2
    ON dt2.DealID = d2.DealID
   AND dt2.TransactType = 14
 INNER JOIN tSecurity s
    ON s.SecurityID = d1.SecurityID
 INNER JOIN tInstitution i
    ON i.InstitutionID = s.InstitutionID      
 WHERE dt1.Date <= @Date
   AND dt2.Date > @Date
   AND d1.ContractFlag = 34
   AND d2.ContractFlag = 35
   AND dr.RelType = 35
   AND d1.LtClientID = 2000
   AND d1.DealType = 0
 GROUP BY i.InstitutionID, s.SecurityID, d1.LtPortfolioID, i.Name, i.INN, s.RegNumber, s.Number                   
                                                           
  
--Проставим КПП, ОГРН и пр.
UPDATE r SET KPP = ISNULL((SELECT kpp.Reuters
                             FROM [dbo].[tReuters] kpp
                            WHERE kpp.InstitutionID = r.InstitutionID
                              AND kpp.TradingSysID = 2    
                              AND kpp.isdefault = 1), '')
           , OGRN = ISNULL((SELECT ogrn.Reuters    
                              FROM [dbo].[tReuters] ogrn
                             WHERE ogrn.InstitutionID = r.InstitutionID
                               AND ogrn.TradingSysID = 1018     
                               AND ogrn.isdefault = 1), '')     
           , OKSM = (SELECT c.CodeISO 
                       FROM tCountry c
                      INNER JOIN tInstitution i
                         ON i.CountryID =  c.CountryID
                      WHERE i.InstitutionID = r.InstitutionID) 
           , FundCode = ISNULL((SELECT f.ISONumber
                                  FROM tFund f
                                 INNER JOIN tSecurity s
                                    ON s.NominalFundID = f.FundID 
                                 WHERE s.SecurityID = r.SecurityID), '')   
           , SecType = ISNULL((SELECT oc.Brief
                                 FROM tObjClsRelation ocr
                                INNER JOIN tObjClassifier oc
                                   ON oc.ObjClassifierID = ocr.ObjClassifierID
                                INNER JOIN tObjClassifier parent 
                                   ON parent.ObjClassifierID = oc.ParentID         
                                WHERE ocr.ObjectID = r.SecurityID
                                  AND parent.Brief = 'Классы ценных бумаг'), '') 
           , DepoAcc = ISNULL((SELECT kt1.Num
                                 FROM [dbo].[tKvitTotal] kt1
                                INNER JOIN [dbo].[tKvitPosition] kp1 
                                   ON kp1.KvitPositionID = kt1.KvitPositionID 
                                  AND kt1.Type IN (1)
                               -- INNER JOIN tPortfolio portf
                               --    ON portf.PortfolioID = kp1.PortfolioID
                                WHERE kt1.Date = @Date 
                                  AND kp1.SecurityID = r.SecurityID
                                  AND kp1.PortfolioID = r.PortfolioID
                                  AND kp1.PortfolioID = @Portf5Kat
                                --  AND portf.Brief = '5-я категория'
                                  ) ,0)                                                                          
  FROM #Rep3 r 

   
 --Вычисление номинала
DELETE FROM pAPI_Security_IDDate 
 WHERE spid = @@SPID

DELETE FROM pAPI_Security_Nominal 
 WHERE spid = @@SPID

INSERT INTO pAPI_Security_IDDate
SELECT @@SPID, SecurityID, @Date
  FROM #Rep3

EXEC API_Sec_FindListNominalByDate

UPDATE r SET Nominal = sn.Nominal
  FROM #Rep3 r 
 INNER JOIN pAPI_Security_Nominal sn
    ON sn.SecurityID = r.SecurityID
   AND sn.SPID = @@SPID
   

-- Количество в РЕПО займ   
UPDATE r SET Repo = Repo - repo.RepoZaem          
           , RepoZaem = repo.RepoZaem
  FROM #Rep3 r
 INNER JOIN (SELECT d1.SecurityID
                  , d1.LtPortfolioID 
                  , SUM(d1.Num) AS RepoZaem                                                                       
               FROM tDealRelation dr 
              INNER JOIN tDeal d1
                 ON d1.DealID = dr.ParentID
              INNER JOIN tDealTransact dt1
                 ON dt1.DealID = d1.DealID
                AND dt1.TransactType = 4
              INNER JOIN tDeal d2
                 ON d2.DealID = dr.ChildID
              INNER JOIN tDealTransact dt2
                 ON dt2.DealID = d2.DealID
                AND dt2.TransactType = 14
              WHERE dt1.Date < @Date
                AND dt2.Date >= @Date
                AND d1.ContractFlag = 34
                AND d2.ContractFlag = 35
                AND dr.RelType = 35
                AND d1.LtClientID = 2000
                AND (d1.CalcNKS & 256) / 256 = 1
                AND d1.DealType = 1
              GROUP BY d1.SecurityID, d1.LtPortfolioID ) repo 
    ON repo.SecurityID = r.SecurityID
   AND repo.LtPortfolioID = r.PortfolioID 
   

-- Результирующий селект
IF EXISTS (SELECT 'X'
             FROM #Rep3)
  SELECT MAX(InstName) AS InstName
       , MAX(INN) AS INN        
       , MAX(KPP) AS KPP 
       , MAX(OGRN) AS OGRN
       , MAX(OKSM) AS OKSM 
       , MAX(SecType) AS SecType 
       , MAX(RegNumber) AS RegNumber
       , MAX(ISIN) AS ISIN  
       , MAX(FundCode) AS FundCode 
       , MAX(Nominal) AS Nominal 
       , SUM(Repo) AS Repo  
       , SUM(RepoZaem) AS RepoZaem 
       , SUM(RepoObr) AS RepoObr
       , SUM(RepoObrZaem) AS RepoObrZaem 
       , SUM(DoverUpr) AS DoverUpr
       , SUM(DoverUprPrav) AS DoverUprPrav
       , SUM(ZalogKO) AS ZalogKO
       , SUM(Zalog3Lic) AS Zalog3Lic
       , SUM(Zalog) AS Zalog 
       , SUM(DepoAcc) AS DepoAcc
       , SUM(OgranKorpD) AS OgranKorpD 
       , SUM(Zapret) AS Zapret
       , SUM(Arest) AS Arest
       , MAX(Comment) AS Comment
    FROM #Rep3
   GROUP BY SecurityID 
   ORDER BY 1 
ELSE     
  SELECT NULL AS InstName
       , NULL AS INN 
       , NULL AS KPP 
       , NULL AS OGRN
       , NULL AS OKSM 
       , NULL AS SecType 
       , NULL AS RegNumber
       , NULL AS ISIN  
       , NULL AS FundCode 
       , 0 AS Nominal 
       , 0 AS Repo  
       , 0 AS RepoZaem 
       , 0 AS RepoObr
       , 0 AS RepoObrZaem 
       , 0 AS DoverUpr
       , 0 AS DoverUprPrav
       , 0 AS ZalogKO
       , 0 AS Zalog3Lic
       , 0 AS Zalog 
       , 0 AS DepoAcc
       , 0 AS OgranKorpD 
       , 0 AS Zapret
       , 0 AS Arest
       , NULL AS Comment
    

}  
      
@SQL_1_4@ = SQL{ 
----------------------------
--  711 форма Раздел 1.4  -- 
----------------------------
DECLARE @KvitID DSIDENTIFIER
      , @Date DSOPERDAY
      , @Portf5Kat DSIDENTIFIER 

SELECT @KvitID = 10000000000
     , @Date = %DATE! 

SELECT @Date = @Date - 1   

--Найдем портфель 5-ой категории
SELECT @Portf5Kat = PortfolioID
  FROM tPortfolio 
 WHERE Brief LIKE '5\%категория' 

-- Заполним параметры дял ВУЗа     
IF EXISTS ( SELECT 'X'
              FROM [dbo].[tInstitution] 
             WHERE InstitutionID = 2000
               AND Brief LIKE '\%ВУЗ-БАНК\%')
  SELECT @KvitID = 19         

 INSERT INTO #Rep1_4 (InstitutionID 
                    , SecurityID    
                    , InstName     
                    , INN          
                    , KPP          
                    , OGRN         
                    , OKSM    
                    , OKVED      
                    , SecType       
                    , RegNumber     
                    , ISIN    
                    , FundCode
                    , SecNum
                    , SumVlozh     
                    , AccBrief 
                    , NKD
                    , RevalOtr
                    , Reval
                    , Portf
                     )
SELECT s.InstitutionID    
     , s.SecurityID   
     , i.Name
     , i.INN
     , ISNULL(kpp.Reuters,'') AS KPP
     , ISNULL(ogrn.Reuters,'') AS OGRN
     , c.CodeISO      
     , ISNULL(okved.Reuters,'') AS OKVED     
     , ISNULL((SELECT oc.Brief
                 FROM tObjClsRelation ocr
                INNER JOIN tObjClassifier oc
                   ON oc.ObjClassifierID = ocr.ObjClassifierID
                INNER JOIN tObjClassifier parent 
                   ON parent.ObjClassifierID = oc.ParentID         
                WHERE ocr.ObjectID = s.SecurityID
                  AND parent.Brief = 'Классы ценных бумаг'), '') AS TypeSec                                     
     , s.RegNumber
     , s.Number    
     , f.ISONumber
     , kt.Num
     , (kt.BuyQty + kt.BuyCoupon + kt.BuyCosts +/*kt.Reval +*/ kt.GrowPremium + kt.GrowCoupon + kt.GrowDiscount) * ISNULL(cr.Course,1) + ISNULL((SELECT SUM(kt1.Reval)
                                                                                                                                                   FROM [dbo].[tKvitTotal] kt1
                                                                                                                                                  INNER JOIN [dbo].[tKvitPosition] kp1 
                                                                                                                                                     ON kp1.KvitPositionID = kt1.KvitPositionID 
                                                                                                                                                    AND kt1.Type IN (1,6)
                                                                                                                                                  WHERE kt1.Date = @Date 
                                                                                                                                                    AND kp1.SecurityID = kp.SecurityID
                                                                                                                                                    AND kp1.PortfolioID = kp.PortfolioID) ,0)   
     , ISNULL((SELECT tr.Brief
                 FROM [dbo].[tInstrRes] tir
                INNER JOIN [dbo].[tResource] tr
                   ON tir.ResourceID = tr.ResourceID
                INNER JOIN [dbo].[tLinkedAccType] la
                   ON la.LinkedAccTypeID = tir.AccType    
                WHERE tir.FormulaID = kp.SecurityID
                  AND tir.PortfolioID = kp.PortfolioID
                  AND ((la.Brief = 'ВложЦБ' AND portf.Brief <> '6 - категория')
                    OR (la.Brief = 'ИнвестПИФ' AND portf.Brief = '6 - категория')
                      )
                  
                  ), '-') 
     , kt.GrowCoupon 
     , CASE WHEN kt.Reval < 0 THEN kt.Reval ELSE 0 END
     , CASE WHEN kt.Reval > 0 THEN kt.Reval ELSE 0 END
     , portf.Brief 
  FROM tKvitTotal kt
 INNER JOIN tKvitPosition kp
    ON kp.KvitPositionID = kt.KvitPositionID
 INNER JOIN tPortfolio portf
    ON portf.PortfolioID = kp.PortfolioID
 INNER JOIN tSecurity s
    ON s.SecurityID = kp.SecurityID   
 INNER JOIN tFund f 
    ON f.FundID = s.NominalFundID   
 INNER JOIN tInstitution i
    ON i.InstitutionID = s.InstitutionID   
  LEFT JOIN [dbo].[tReuters] kpp
    ON i.InstitutionID = kpp.InstitutionID
   AND kpp.TradingSysID = 2    
   AND kpp.isdefault = 1  
  LEFT JOIN [dbo].[tReuters] ogrn
    ON i.InstitutionID = ogrn.InstitutionID
   AND ogrn.TradingSysID = 1018     
   AND ogrn.isdefault = 1  
  LEFT JOIN [dbo].[tReuters] okved
    ON i.InstitutionID = okved.InstitutionID
   AND okved.TradingSysID = 8   
   AND okved.isdefault = 1       
  LEFT JOIN tCountry c
    ON c.CountryID = i.CountryID
  LEFT JOIN tCurrencyRate  cr
    ON cr.ObjectID = s.NominalFundID
   AND cr.Date_Time = kt.Date  
 WHERE kt.Date = @Date 
   AND kt.Num > 0
   AND kt.Type = 1
   --AND portf.Brief <> '5-я категория'
   AND portf.PortfolioID <> @Portf5Kat
   AND kp.KvitID = @KvitID                              
   
-- Удалим банкротов
DELETE FROM #Rep1_4   
 WHERE AccBrief LIKE '50505\%' 
 
--Для след. категорий заполнять 105 графу не нужно   
UPDATE r SET OKVED = ''
  FROM #Rep1_4 r
 WHERE SecType IN ('BON2','BON5','SHS7','SHS8')    

 --Вычисление номинала
DELETE FROM pAPI_Security_IDDate 
 WHERE spid = @@SPID

DELETE FROM pAPI_Security_Nominal 
 WHERE spid = @@SPID

INSERT INTO pAPI_Security_IDDate
SELECT @@SPID, SecurityID, @Date
  FROM #Rep1_4

EXEC API_Sec_FindListNominalByDate

UPDATE r SET Nominal = sn.Nominal
  FROM #rep1_4 r 
 INNER JOIN pAPI_Security_Nominal sn
    ON sn.SecurityID = r.SecurityID
   AND sn.SPID = @@SPID 
 
 
-- Категория качества
DELETE pAPI_Inst_ListID
  FROM pAPI_Inst_ListID  #M_ROWLOCK_INDEX(XPKpAPI_Inst_ListID)
 WHERE spid = @@SPID

INSERT pAPI_Inst_ListID
SELECT @@SPID
     , InstitutionID
  FROM #rep1_4               
 GROUP BY InstitutionID              

 
EXEC FCD_TRE_Inst_FndLstQualForDate
     @Date       = @Date
   , @GroupBrief = 'Резерв232'

-- берем наименование группы риска эммитента, если не нашли классиф на ЦБ и ГФИ     
UPDATE #rep1_4 
   SET KatKach = p.Name
     , Reserv = p.Norm      
  FROM #rep1_4     t     
 INNER JOIN tTRE_Security_Sync     s   
    ON s.SecurityID = t.SecurityID    
 INNER JOIN pAPI_Inst_ListQuality  p   
    ON p.spid          = @@SPID
   AND p.InstitutionID = s.InstitutionID
   AND p.Date          = (SELECT MAX(pp.Date)
                            FROM pAPI_Inst_ListQuality  pp  
                           WHERE pp.spid          = @@SPID
                             AND pp.InstitutionID = s.InstitutionID
                             AND pp.Date         <= @Date
                          )

-- По умолчанию ставим 1 категорию
UPDATE #rep1_4  
   SET KatKach = 'I группа риска'
     , Reserv   = 0      
  FROM #rep1_4                t   #M_NOLOCK   
 WHERE ISNULL(t.KatKach, '') = ''  

-- Посчитаем резерв 
UPDATE #rep1_4  
   SET Reserv   = Reserv /100 * SumVlozh
  FROM #rep1_4                t   #M_NOLOCK    
    
    
-- Результирующий селект
IF EXISTS(SELECT 'X'
            FROM #Rep1_4)   
   SELECT InstName
        , INN
        , OGRN
        , KPP                          
        , OKSM
        , OKVED
        , SecType
        , RegNumber
        , ISIN
        , FundCode
        , SecNum
        , Nominal
        , ROUND(SumVlozh/1000, 2) AS SumVlozh
        , SUBSTRING(AccBrief,1,5) AS AccBrief
        , NKD 
        , ROUND(RevalOtr/1000, 2) AS RevalOtr
        , ROUND(Reval/1000, 2) AS Reval
        , Portf
        , KatKach
        , ROUND(Reserv/1000, 2) AS Reserv
     FROM #Rep1_4
ELSE
   SELECT NULL AS InstName
        , NULL AS INN
        , NULL AS OGRN
        , NULL AS KPP                          
        , NULL AS OKSM
        , NULL AS OKVED        
        , NULL AS SecType
        , NULL AS RegNumber
        , NULL AS ISIN
        , NULL AS FundCode
        , 0 AS SecNum 
        , 0 AS Nominal
        , 0 AS SumVlozh
        , NULL AS AccBrief
        , 0 AS NKD 
        , 0 AS RevalOtr
        , 0 AS Reval
        , NULL AS Portf
        , NULL AS KatKach
        , 0 AS Reserv
}     

@SQL_1_3@ = SQL{ 
----------------------------
--  711 форма Раздел 1.3  -- 
----------------------------
DECLARE @KvitID DSIDENTIFIER
      , @Date   DSOPERDAY
      , @Portf5Kat DSIDENTIFIER

SELECT @KvitID = 10000000000
     , @Date = %DATE! 
     
SELECT @Date = @Date - 1         

--Найдем портфель 5-ой категории
SELECT @Portf5Kat = PortfolioID
  FROM tPortfolio 
 WHERE Brief LIKE '5\%категория'

-- Заполним параметры для ВУЗа     
IF EXISTS ( SELECT 'X'
              FROM [dbo].[tInstitution] 
             WHERE InstitutionID = 2000
               AND Brief LIKE '\%ВУЗ-БАНК\%')
